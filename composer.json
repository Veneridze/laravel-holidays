{
    "name": "veneridze/laravel-holidays",
    "description": "Check if current date is holiday",
    "require": {
        "php": ">=8.2",
        "laravel/framework": "^11.0",
        "spatie/laravel-package-tools": "^1.16"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Veneridze\\LaravelHoliday\\HolidayProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Veneridze\\LaravelHoliday\\": "src/"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "baseline": "vendor/bin/phpstan analyse --generate-baseline",
        "test": "vendor/bin/pest"
    },
    "authors": [
        {
            "name": "Veneridze",
            "email": "viacheslav.kopaev@yandex.ru"
        }
    ],
    "require-dev": {
        "overtrue/phplint": "^9.5",
        "phpstan/phpstan": "^1.12"
    }
}
